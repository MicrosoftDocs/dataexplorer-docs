{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "_themes/**",
          "_themes.pdf/**",
          "**/docfx.json",
          "_repo.en-us/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices.md"
        ],
        "src": "./",
        "group": "gr-kusto"
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.svg",
          "**/*.png",
          "**/*.jpg",
          "**/*.jpeg",
          "**/*.gif"
        ],
        "exclude": [
          "**/obj/**",
          "_themes/**",
          "_themes.pdf/**",
          "**/docfx.json",
          "_repo.en-us/**"
        ],
        "src": "./",
        "group": "gr-kusto"
      }
    ],
    "groups": {
      "gr-kusto": {
        "moniker_range": "microsoft-fabric || azure-data-explorer || azure-monitor || microsoft-sentinel ",
        "dest": "kusto-docs"
      }
    },
